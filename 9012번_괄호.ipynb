    "n = int(input())\n",
    "\n",
    "for _ in range(n):\n",
    "    o=input()\n",
    "    s=0\n",
    "    for i in o:\n",
    "        if i==\"(\": s +=1\n",
    "        else : s -=1\n",
    "        if s<0: print(\"NO\"); break\n",
    "    else:\n",
    "        if s !=0: print(\"NO\"); continue\n",
    "        else : print(\"YES\")"
   ]
